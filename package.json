{
  "name": "hello-solana",
  "version": "1.0.0",
  "description": "Solana SPL Token operations demo",
  "main": "dist/spl_token_operations.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/spl_token_operations.js",
    "dev": "ts-node src/spl_token_operations.ts",
    "build:kit": "tsc",
    "start:kit": "node dist/spl_token_operations_kit.js",
    "dev:kit": "ts-node src/spl_token_operations_kit.ts",
    "build:raw": "tsc",
    "start:raw": "node dist/spl_token_operations_raw.js",
    "dev:raw": "ts-node src/spl_token_operations_raw.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solana",
    "spl-token",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.95.4",
    "@solana/spl-token": "^0.4.9",
    "@solana/kit": "^3.0.2",
    "@solana-program/token": "^0.6.0",
    "@solana-program/system": "^0.8.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  }
}
